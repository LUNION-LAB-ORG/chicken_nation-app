{
  "expo": {
    "scheme": "unionlab",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/*"
    ],
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Urbanist-Regular.ttf",
            "./src/assets/fonts/Urbanist-Bold.ttf",
            "./src/assets/fonts/Urbanist-Medium.ttf",
            "./src/assets/fonts/Urbanist-Light.ttf",
            "./src/assets/fonts/Urbanist-Regular.ttf",
            "./src/assets/fonts/Blocklyn-Grunge.otf",
            "./src/assets/fonts/Blocklyn-Condensed.otf",
            "./src/assets/fonts/SofiaProRegular.ttf",
            "./src/assets/fonts/SofiaProMedium.ttf",
            "./src/assets/fonts/SofiaProBold.ttf",
            "./src/assets/fonts/SofiaProLight.ttf",
            "./src/assets/fonts/SofiaProSemiBold.ttf"
          ]
        }
      ],
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "splash": {
      "image": "./src/assets/icons/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.chickennation.app",
      "versionCode": 1,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_API_KEY"
        }
      }
    },
    "ios": {
      "splash": {
        "image": "./src/assets/icons/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF"
      }
    },
    "name": "chicken-nation",
    "slug": "chicken-nation",
    "version": "1.0.0",
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "12d22b15-cc96-4f3a-82d4-219f16de9176"
      }
    },
    "owner": "ambroisek"
  }
}
